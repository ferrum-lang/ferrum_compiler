use ::fe::print

pub fn main()
    mut x1 = false
    mut x2 = false
    mut x3 = false

    print("Starting to loop...")

    loop
        print("LOOPING")

        if not x1
            x1 = true
        else if not x2
            x2 = true
        else if not x3
            x3 = true
        else
            break
        ;
    ;

    print("Done looping :)")
;
