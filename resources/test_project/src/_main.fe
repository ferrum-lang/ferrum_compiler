use ::fe::print

pub fn main()
    const is_funny = true
    const is_cool = true

    const response = if' is_funny
        print("you're funny ...")

        if is_cool
            print("Wow you're lucky")

            then' "HAHA You're Amazing!"
        else
            then' "hah"
        ;

        // print("Not reachable")

    else'a
        print("you're not so funny ...")

        if is_cool
            then'a "Whatevs"
        ;

        print("Not so lucky...")
        then "Ew times {42}."

        // print("Not reachable")
    ;

    print("\nRESPONSE = {response}")
;

